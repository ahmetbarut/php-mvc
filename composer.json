{
    "name": "ahmetbarut/rain",
    "description": "Rain framework",
    "type": "project",
    "require": {
        "ahmetbarut/php-router": "dev-master",
        "psr/container": "2.0.x-dev",
        "ahmetbarut/translation": "dev-master",
        "ahmetbarut/validation": "dev-master",
        "ahmetbarut/view-render": "dev-master",
        "symfony/console": "6.0.x-dev",
        "symfony/finder": "6.0.x-dev"
    },
    "require-dev": {
        "symfony/var-dumper": "6.0.x-dev",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Core\\": "core/"
        },
        "files": [
            "core/Helper/helpers.php"
        ]
    },
    "autoload-dev": {
        "Test\\": "tests/"
    },
    "minimum-stability": "dev"
}
